{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "build": {
    "beforeDevCommand": "pnpm --filter @upryzing/lavender dev --port 6173",
    "devPath": "http://localhost:6173",
    "distDir": "../handoff",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "Upryzing",
    "version": "0.1.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      },
      "window": {
        "all": false,
        "close": true,
        "hide": true,
        "show": true,
        "maximize": true,
        "minimize": true,
        "unmaximize": true,
        "unminimize": true,
        "startDragging": true
      }
    },
    "bundle": {
      "active": true,
      "category": "SocialNetworking",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "../../client/assets/icons/favicon-16x16.png",
        "../../client/assets/icons/android-chrome-512x512.png",
        "../../client/assets/icons/icon.icns",
        "../../client/assets/logo_round.png",
        "../../client/assets/icons/icon.ico"
      ],
      "identifier": "app.upryzing.desktop",
      "longDescription": "Your conversations, your way. Connect with Upryzing.",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "Your conversations, your way.",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 720,
        "resizable": true,
        "title": "Upryzing",
        "width": 1280,
        "visible": true,
        "center": true,
        "focus": true,
        "decorations": false,
        "titleBarStyle": "Overlay",
        "minWidth": 940,
        "minHeight": 500
      }
    ],
    "systemTray": {
      "iconPath": "../../client/assets/icons/tray/trayIcon.png",
      "iconAsTemplate": true,
      "title": "Upryzing"
    }
  }
}